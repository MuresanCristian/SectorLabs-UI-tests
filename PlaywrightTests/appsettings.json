{
  "App": {
    "BaseUrl": "https://www.{{param}}"
  },
  "Locators": {
    "Filters": {
      "ExpandFilters": "//div[@data-testid='little-search']",
      "ExpandExtraFilters": "//button[contains(@style,'filter')]",
      "FillInputUnderLabel": "//div[text()='{{param}}']/parent::div/input",
      "SearchButton": "//button[@data-testid='structured-search-input-search-button']",
      "AddGuestsBasedOnGuestType": "//button[@data-testid='stepper-{{param}}-increase-button']",
      "CalendarDaySelector": "//div[@data-testid='calendar-day-{{param}}']",
      "ExpandGuestsFilter": "//div[@data-testid='structured-search-input-field-guests-button']",
      "FilterValues": "//span[contains(., '{{param}}')]/parent::button/div[1]",
      "NumberOfPropertiesInFirstPage": "//div[@itemprop='itemListElement']",
      "IterateEachFirstPageProperty": "//div[@itemprop='itemListElement']/meta[@content='{{param}}']/parent::div",
      "PropertyAllowedGuests": "//h2/parent::div/parent::div/ol/li[1]/span[1]",
      "FilterNumberOfBedrooms": "//div[@aria-label='Bedrooms']/div/div/div/div/div[@data-testid='menuItemButton-{{param}}']",
      "AmenititesShowMore": "//h2[text()='Amenities']/parent::div/parent::section/div/div/div/button",
      "AmenititesSelection": "//div[text()='{{param}}']",
      "ApplyExtraFiltersButton": "//a[@aria-live='polite']",
      "NumberOfBedroomsInDashboard": "//div[@itemprop='itemListElement']/meta[@content='{{param}}']/parent::div/div/div/div/div/div/div[4]/span[contains(.,'bedroom')]",
      "OpenAllPropertyAmenities": "//button[contains(.,'amenities')]",
      "AllFacilitiesAmenitites": "//h3[contains(.,'facilities')]/parent::div/parent::div/div[{{param}}]/div/div",
      "TranslationButtonModal": "//header[@data-testid='translation-announce-modal']/parent::div/div/div/div/button",
      "TranslationToggle": "//button[@aria-labelledby='auto_translate_switch']",
      "PropertyMapPinDinamicSelection": "//span[text()='selected']",
      "PropertyMapPinDinamicBackground": "//span[text()='selected']/parent::div/div",
      "GetAllPinsBackgroundColor": "//div[{{param}}]/div/button/div/div/div/span[contains(@aria-label,'Map marker of the listing')]/parent::div/parent::div",
      "PropertyTitleMapCard": "//div[contains(@style ,'z-index: 10000;')]/div/div/div/div[1]/div[1]",
      "PropertyDescriptionMapCard": "//div[contains(@style ,'z-index: 10000;')]/div/div/div/div[1]/div/span",
      "FirstPropertyTitle": "//div[@itemprop='itemListElement']/meta[@content='1']/parent::div/div/div/div/div/div[1]/div[1]",
      "FirstPropertyDescription": "//div[@itemprop='itemListElement']/meta[@content='1']/parent::div/div/div/div/div/div[1]/div[3]/span"
    }
  }
}
